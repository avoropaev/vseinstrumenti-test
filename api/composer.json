{
    "name": "avoropaev/vseinstrumenti-test",
    "type": "project",
    "license": "MIT License",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "doctrine/orm": "^2.7",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/routing": "^5.0"
    },
    "require-dev": {
        "overtrue/phplint": "^1.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm"
    }
}
